<?php

use Illuminate\Support\Facades\Route;
use App\Http\Controllers\ProfileController;
use App\Http\Controllers\Admin\AdminDashboardController;
use App\Http\Controllers\Admin\AdminUserController;
use App\Http\Controllers\User\UserDashboardController;

/*
|--------------------------------------------------------------------------
| Web Routes
|--------------------------------------------------------------------------
|
| Burada web rotalarÄ±nÄ± tanÄ±mlarsÄ±n. TÃ¼m rotalar 'web' middleware grubundadÄ±r.
| Bu dosya RouteServiceProvider tarafÄ±ndan otomatik yÃ¼klenir.
|
*/

// 1. Anasayfa: root URL'den login sayfasÄ±na yÃ¶nlendirme
Route::get('/', function () {
    return redirect()->route('login');
});

// 2. Admin panel rotalarÄ± (yetkisi olan kullanÄ±cÄ±lar)
Route::middleware(['auth', 'verified', 'isAdmin'])->prefix('admin')->name('admin.')->group(function () {
    // Admin dashboard
    Route::get('/dashboard', [AdminDashboardController::class, 'index'])->name('dashboard');

    // Admin kullanÄ±cÄ± yÃ¶netimi CRUD
    Route::get('/users', [AdminUserController::class, 'index'])->name('user.index');
    Route::get('/users/create', [AdminUserController::class, 'create'])->name('user.create'); // Yeni kullanÄ±cÄ± formu
    Route::post('/users', [AdminUserController::class, 'store'])->name('user.store'); // Yeni kullanÄ±cÄ± kaydet
    Route::get('/users/{user}/edit', [AdminUserController::class, 'edit'])->name('user.edit'); // DÃ¼zenleme formu
    Route::put('/users/{user}', [AdminUserController::class, 'update'])->name('user.update'); // GÃ¼ncelle
    Route::delete('/users/{user}', [AdminUserController::class, 'destroy'])->name('user.destroy'); // Sil
});

// 3. KullanÄ±cÄ± panel rotalarÄ±
Route::middleware(['auth', 'verified'])->prefix('user')->name('user.')->group(function () {
    // KullanÄ±cÄ± dashboard
    Route::get('/dashboard', [UserDashboardController::class, 'index'])->name('dashboard');
});

// 4. Profil iÅŸlemleri (giriÅŸ yapmÄ±ÅŸ tÃ¼m kullanÄ±cÄ±lar iÃ§in)
Route::middleware('auth')->group(function () {
    Route::get('/profile', [ProfileController::class, 'edit'])->name('profile.edit');
    Route::patch('/profile', [ProfileController::class, 'update'])->name('profile.update');
    Route::delete('/profile', [ProfileController::class, 'destroy'])->name('profile.destroy');
});

// 5. Laravel Auth rotalarÄ± (login, register, password reset vb.)
require __DIR__.'/auth.php';
Ã–nemli notlar:

